{
  "name": "automit",
  "displayName": "Automit",
  "description": "AI generated commit messags",
  "publisher": "MunozVscodeExtensions",
  "icon": "media/automit128.png",
  "keywords": [
    "VSCode",
    "Extension",
    "Git",
    "Commit Messages",
    "Automation",
    "Productivity",
    "Code Quality",
    "Development Workflow",
    "Version Control",
    "Commit Templates",
    "Code Changes",
    "Simplification",
    "API Key Management",
    "Configuration",
    "Automation Tools",
    "Developer Tools",
    "Git Integration",
    "Coding Efficiency",
    "Codebase Maintenance",
    "Customization"
  ],
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:automit-sidebar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "automit.commitView",
          "name": "Automit"
        }
      ]
    },
    "commands": [
      {
        "command": "automit.automit",
        "title": "Generate commit message"
      },
      {
        "command": "automit.SourceControlCommand",
        "title": "Start Automit"
      },
      {
        "command": "automit.newKey",
        "title": "Enter New API Key"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "openai": "^4.12.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexJH221/automit"
  }
}
